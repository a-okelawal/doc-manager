{
  "name": "check3",
  "version": "1.0.0",
  "description": "An app used to manage documents",
  "main": "server.js",
  "scripts": {
    "set-db": "dropdb --if-exists database_test && createdb database_test",
    "seed": "NODE_ENV=test node seeders/seedData.js",
    "moch": "istanbul cover node_modules/.bin/_mocha --reporter lcovonly -- -- test/*",
    "test": "npm run set-db && npm run seed && NODE_ENV=test npm run moch",
    "start": "NODE_ENV=test npm run seed && node server.js",
    "coverage": "rm -rf coverage/",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-abisoye/doc-manager.git"
  },
  "author": "Abisoye Oke-lawal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-abisoye/doc-manager/issues"
  },
  "homepage": "https://github.com/andela-abisoye/doc-manager#readme",
  "devDependencies": {
    "coveralls": "^2.11.14",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "mocha": "^3.1.2",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cookie-session": "^1.2.0",
    "crypto-js": "^3.1.8",
    "jsonwebtoken": "^7.1.9",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.76.0",
    "sequelize": "^3.24.5"
  }
}
